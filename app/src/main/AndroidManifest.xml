<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.team.imagemarker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.HomeActivity" />
        <activity android:name=".activitys.UserCenterActivity" />
        <activity android:name=".activitys.UpdateUserMessageActivity" />
        <activity android:name=".activitys.UserHistoryRecord"/>
        <activity android:name=".activitys.UserMissionActivity" />
    </application>

</manifest>